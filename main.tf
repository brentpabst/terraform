###############################################################################
# Remote State:
###############################################################################
terraform {
  backend "remote" {
    hostname     = "app.terraform.io"
    organization = "brentpabst"

    workspaces {
      name = "terraform"
    }
  }
}

###############################################################################
# Providers:
###############################################################################
provider "aws" {
  profile = "default"
  region  = "us-east-1"
}
